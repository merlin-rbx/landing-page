{
	"plugins": ["unused-imports", "prettier"],
	"extends": [
		"next",
		"next/core-web-vitals",
		"plugin:tailwind/recommended",
		"plugin:prettier/recommended"
	],
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"semi": true,
				"trailingComma": "all",
				"singleQuote": false,
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": false,
				"endOfLine": "auto"
			}
		]
	}
}
